{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "allowJs": true,
    "importsNotUsedAsValues": "error",
    "ignoreDeprecations": "5.0",
    "forceConsistentCasingInFileNames": false
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}